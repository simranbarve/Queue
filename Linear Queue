class Queue:

    def __init__(self, queue_size = 7):
        self.queue_size = queue_size
        self.queue = [None] * queue_size
        self.rear = -1
        self.front = -1

    def isempty(self):
        if self.rear and self.front == -1:
            return True
        else:
            return False

    def isfull(self):
        if self.rear = self.queue_size:
            return True
        else:
            return False

    def enqueue(self, value):
        if not self.isfull():
            if self.isempty():
                self.rear += 1
                self.front += 1
                self.stack[self.rear] =  value
            else:
                self.rear += 1
                self.stack[self.rear] =  value
            return True
        else:
            return False

    def dequeue(self,value):
        if not self.isfull() and not self.isempty():
            return self.queue[self.rear]
        return False
